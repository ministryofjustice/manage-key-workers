  "scripts": {
    "start": "DISABLE_WEBPACK=true node dist/backend/index.js",
    "start:dev": "nodemon",
    "start-feature": "npm run compile-sass && export $(cat $npm_config_env) && npm run start:dev",
    "start-feature-no-webpack": "export $(cat $npm_config_env) && npm run start",
    "start-prod": "NODE_ENV=production npm run start",
    "build": "rm -rf dist && npm run build:frontend && npm run build:backend && npm run build:copy",
    "build:backend": "tsc",
    "build:frontend": "webpack --mode=production && npm run compile-sass",
    "build:copy": "cp -R views dist/views && cp -R build dist/build",
    "build:analyse": "BUNDLE_ANALYSE=true npm run build:frontend",
    "test": "jest --env=jsdom",
    "test-watch": "jest --env=jsdom --watchAll",
    "record-build-info": "node scripts/record-build-info",
    "plant-beanstalk": "node scripts/plant-beanstalk.js",
    "lint": "node node_modules/eslint/bin/eslint.js --max-warnings 0 src backend",
    "verify": "npm run lint && npm test",
    "security_audit": "npx audit-ci --config audit-ci.json",
    "int-test": "cypress run",
    "int-test-ui": "cypress open",
    "compile-sass": "sass --load-path=. ./sass:./build/static/stylesheets --style compressed",
    "compile-sass-watch": "npm run compile-sass && sass --load-path=. ./sass:./build/static/stylesheets --watch --style compressed",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{ts,js,json,css}": [
      "prettier --write"
    ],
    "*.{ts,js}": [
      "eslint --cache --fix"
    ]
  },
  "nodemonConfig": {
    "watch": [
      "backend"
    ],
    "ext": "ts,tsx,json,js,jsx,mjs",
    "ignore": [
      "**/*.test.*"
    ],
    "exec": "ts-node ./backend/index.js | bunyan -o simple"
  }
}
